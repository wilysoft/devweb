<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <table>
      <tbody><tr>
        <th colspan="2">Registro</th>
        </tr>
      <tr>
        <td class="a1" width="23%"><div><%= f.label :iduser, "Codigo Usuario" %></div></td>
        <td class="a1" width="77%"><%= f.text_field :iduser, autofocus: true %></td>
        </tr>
      <tr>
        <td class="a1" ><%= f.label :password, "ContraseÃ±a" %> <% if @validatable %><i>(<%= @minimum_password_length %> characters minimum)</i><% end %></td>
        <td class="a1" ><%= f.password_field :password, autocomplete: "off" %></td>
      </tr>
      <tr>
        <td class="a1" ><%= f.label :password_confirmation, "ContraseÃ±a" %></td>
        <td class="a1" ><%= f.password_field :password_confirmation, autocomplete: "off" %></td>
      </tr>
      <tr>
        <td class="a1" ><%= f.label :name, "Nombres" %></td>
        <td class="a1" ><%= f.text_field :name, autofocus: true %></td>
      </tr>      
      <tr>
        <td class="a1" ><%= f.label :lastname, "Apellidos" %></td>
        <td class="a1" ><%= f.text_field :lastname, autofocus: true %></td>
      </tr>   
      <tr>
        <td class="a1" ><%= f.label :birthday, "Fecha Nacimiento" %></td>
        <td class="a1" ><%= f.date_field :birthday, autofocus: true %></td>
      </tr>   
      <tr>
        <td class="a1" ><%= f.label :email, "Correo" %></td>
        <td class="a1" ><%= f.email_field :email, autofocus: true %></td>
      </tr>    
      <tr>
        <td class="a1" ><%= f.label :address, "Direccion" %></td>
        <td class="a1" ><%= f.text_field :address, autofocus: true %></td>
      </tr>                              
      <tr>
        <td  class="a1" colspan="2"><%= f.submit "Registrar" %></td>
      </tr>

    </tbody></table>
<% end %>

<%= render "devise/shared/links" %>
