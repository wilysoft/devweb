<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
	<div style="border: 2pt solid #fbb; text-align: center;">
		<table align="center" width="140">
			<tbody><tr>
				<th colspan="2">Ingresar</th>
				</tr>
			<tr>
				<td class="a1" width="60"><div><%= f.label :iduser, "Usuario :" %></div></td>
				<td class="a1" width="80"><%= f.text_field :iduser, autofocus: true %></td>
			</tr>
			<tr>
				<td class="a1" ><%= f.label :password, "ContraseÃ±a : " %></td>
				<td class="a1" ><%= f.password_field :password, autocomplete: "off" %></td>
			</tr>
			<tr>
				<td  class="a1" ><%= f.submit "Ingresar" %></td>
				<td  class="a1" >
					<%= image_tag "facebook_logo.jpg", :alt => "Facebook", :title => "Facebook"%>
					<%= image_tag "twitter_logo.jpg", :alt => "Twitter", :title => "Twitter"%>
					<%= image_tag "google_mas_logo.jpg", :alt => "Google +", :title => "Google +"%>
				</td>
			</tr>

			</tbody>
		</table>
	</div>
<% end %>


